{
  "input": {
    "columns": [
      "col1",
      "col2",
      "col3",
      "col4"
    ]
  },
  "preprocessing": [
    {
      "one_hot_encoding": {
        "field": "col1",
        "hot_map": {
          "male": "col1_male",
          "female": "col2_female"
        }
      }
    },
    {
      "target_mean_encoding": {
        "field": "col2",
        "target_map": {
          "S": 5.0,
          "M": 10.0,
          "L": 20
        }
      }
    },
    {
      "frequency_encoding": {
        "field": "col3",
        "frequency_map": {
          "none": 0.8,
          "true": 0.10,
          "false": 0.10
        }
      }
    }
  ]
}
{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://raw.githubusercontent.com/elastic/ml_json_schemas/master/schemas/trained_model.schema.json",
  "title": "trained_model",
  "description": "Details of the model evaluation step.",
  "type": "object",
  "definitions": {
    "numeric_relationship": {
      "$id": "#numeric_relationship",
      "type": "string",
      "enum": [
        "lte"
      ]
    },
    "basic_evaluator": {
      "type": "object",
      "$id": "#basic_evaluator",
      "properties": {
        "feature_names": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "classification_labels": {
          "items": {
            "type": "string"
          }
        },
        "target_type": {
          "type": "string",
          "enum": [
            "classification",
            "regression"
          ]
        }
      },
      "additionalProperties": false
    },
    "tree_node": {
      "$id": "#tree_node",
      "type": "object",
      "properties": {
        "node_index": {
          "type": "integer"
        },
        "split_feature": {
          "type": "integer"
        },
        "split_gain": {
          "type": "number"
        },
        "threshold": {
          "type": "number"
        },
        "decision_type": {
          "$ref": "#/definitions/numeric_relationship"
        },
        "default_left": {
          "type": "boolean"
        },
        "left_child": {
          "type": "integer"
        },
        "right_child": {
          "type": "integer"
        }
      },
      "required": [
        "node_index",
        "split_feature",
        "threshold",
        "decision_type",
        "default_left",
        "left_child",
        "right_child"
      ],
      "additionalProperties": false
    },
    "tree_leaf": {
      "$id": "#tree_leaf",
      "type": "object",
      "properties": {
        "node_index": {
          "type": "integer"
        },
        "leaf_value": {
          "type": "number"
        }
      },
      "required": [
        "node_index",
        "leaf_value"
      ],
      "additionalProperties": false
    },
    "tree": {
      "$id": "#tree",
      "properties": {
        "feature_names": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "tree_structure": {
          "type": "array",
          "items": {
            "oneOf": [
              {
                "$ref": "#/definitions/tree_node"
              },
              {
                "$ref": "#/definitions/tree_leaf"
              }
            ]
          }
        },
        "classification_labels": {
          "items": {
            "type": "string"
          }
        },
        "target_type": {
          "type": "string",
          "enum": [
            "classification",
            "regression"
          ]
        }
      },
      "required": [
        "tree_structure",
        "feature_names",
        "target_type"
      ]
    },
    "ensemble": {
      "$id": "#ensemble",
      "properties": {
        "feature_names": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "trained_models": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "tree": {
                "$ref": "#/definitions/tree"
              }
            },
            "required": [
              "tree"
            ],
            "additionalProperties": false
          }
        },
        "classification_labels": {
          "items": {
            "type": "string"
          }
        },
        "target_type": {
          "type": "string",
          "enum": [
            "classification",
            "regression"
          ]
        },
        "aggregate_output": {
          "type": "object",
          "oneOf": [
            {
              "type": "object",
              "properties": {
                "weighted_sum": {
                  "type": "object",
                  "properties": {
                    "weights": {
                      "type": "array",
                      "items": {
                        "type": "number"
                      }
                    }
                  },
                  "required": [
                    "weights"
                  ]
                }
              },
              "required": [
                "weighted_sum"
              ]
            },
            {
              "type": "object",
              "properties": {
                "weighted_mode": {
                  "type": "object",
                  "description": "Allows to used (weighted) majority vote for classification.",
                  "properties": {
                    "weights": {
                      "type": "array",
                      "items": {
                        "type": "number"
                      }
                    }
                  },
                  "required": [
                    "weights"
                  ]
                }
              },
              "required": [
                "weighted_mode"
              ]
            },
            {
              "type": "object",
              "properties": {
                "logistic_regression": {
                  "type": "object",
                  "description": "Given a weights vector $\\vec{w}$ as a parameter and an output vector from the ensemble $\\vec{x}$, it computes the logistic regression function $1/(1 + \\exp(-\\vec{w}^T \\vec{x}))$.",
                  "properties": {
                    "weights": {
                      "type": "array",
                      "items": {
                        "type": "number"
                      }
                    }
                  },
                  "required": [
                    "weights"
                  ]
                }
              },
              "required": [
                "logistic_regression"
              ]
            }
          ]
        }
      },
      "required": [
        "feature_names",
        "trained_models",
        "target_type",
        "aggregate_output"
      ],
      "additionalProperties": false
    }
  },
  "oneOf": [
    {
      "properties": {
        "ensemble": {
          "$ref": "#/definitions/ensemble"
        }
      },
      "required": [
        "ensemble"
      ],
      "additionalProperties": false
    },
    {
      "properties": {
        "tree": {
          "$ref": "#/definitions/tree"
        }
      },
      "required": [
        "tree"
      ],
      "additionalProperties": false
    }
  ]
}

{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "$id": "https://raw.githubusercontent.com/elastic/ml_json_schemas/master/schemas/definition.schema.json",
  "title": "definition",
  "description": "Technical details required for model evaluation.",
  "type": "object",
  "properties": {
    "input": {
      "description": "Information related to the input.",
      "$ref": "https://raw.githubusercontent.com/elastic/ml_json_schemas/master/schemas/input.schema.json"
    },
    "preprocessors": {
      "description": "Optional step for pre-processing data, e.g. vector embedding, one-hot-encoding, etc.",
      "$ref": "https://raw.githubusercontent.com/elastic/ml_json_schemas/master/schemas/preprocessors.schema.json"
    },
    "trained_model": {
      "description": "Details of the model evaluation step with a trained_model.",
      "$ref": "https://raw.githubusercontent.com/elastic/ml_json_schemas/master/schemas/trained_model.schema.json"
    }
  },
  "required": [
    "input",
    "trained_model"
  ],
  "additionalProperties": false
}
